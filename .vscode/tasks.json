{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Active File",
			"type": "shell",
			"command": "${workspaceFolder}/run.sh",
			"args": [
				"-n",
				"${file}"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}